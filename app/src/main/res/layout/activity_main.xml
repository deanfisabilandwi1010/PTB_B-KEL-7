<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/top"
        android:layout_width="416dp"
        android:layout_height="78dp"
        android:layout_centerHorizontal="true"
        android:scaleType="centerCrop"
        android:src="@drawable/top"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/view2"
        android:layout_width="369dp"
        android:layout_height="238dp"
        android:background="@drawable/shape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/top"
        app:layout_constraintVertical_bias="0.106" />

    <TextView
        android:id="@+id/dateDetail"
        android:layout_width="146dp"
        android:layout_height="22dp"
        android:fontFamily="sans-serif-black"
        android:text="Telp : 08192102211"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="238dp"
        android:layout_height="20dp"
        android:fontFamily="sans-serif-black"
        android:text="Alamat : Komp Belanti UNAND B2"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.324"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/judulDetail"
        app:layout_constraintVertical_bias="0.02" />

    <TextView
        android:id="@+id/judulDetail"
        android:layout_width="218dp"
        android:layout_height="18dp"
        android:fontFamily="sans-serif-black"
        android:text="Full Name : Dr.Richard Lee Sin"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.21"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textNamaDosen"
        app:layout_constraintVertical_bias="0.015" />

    <TextView
        android:id="@+id/textNamaDosen"
        android:layout_width="218dp"
        android:layout_height="19dp"
        android:fontFamily="sans-serif-black"
        android:text="E-MAIL : Kaprodi@gmail.com"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.21"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.244" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="98dp"
        android:layout_height="178dp"
        android:src="@drawable/fotodosen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.118"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.018" />

    <TextView
        android:id="@+id/textnamaUser"
        android:layout_width="101dp"
        android:layout_height="15dp"
        android:fontFamily="sans-serif-black"
        android:text="Dr.Richard Lee"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="10dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.109"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="101dp"
        android:layout_height="15dp"
        android:fontFamily="sans-serif-black"
        android:text="19580812xxxx"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textnamaUser"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="21dp"
        android:fontFamily="sans-serif-black"
        android:text="Profile"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.091"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/top"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/seminarreq"
        android:onClick="permintaanSeminar"
        android:layout_width="151dp"
        android:layout_height="53dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/buttonshapemain"
        android:fontFamily="sans-serif-condensed"
        android:text="Permintaan Seminar"
        android:textColor="@color/white"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/sidreq"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:layout_constraintVertical_bias="0.233" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/sidreq"
        android:onClick="permintaanSidang"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/buttonshapemain"
        android:fontFamily="sans-serif-condensed"
        android:text="Permintaan Sidang"
        android:textColor="@color/white"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:layout_constraintVertical_bias="0.231" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_jdwlsidang"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="34dp"
        android:background="@drawable/buttonshapemain"
        android:fontFamily="sans-serif-condensed"
        android:onClick="jdwlsidang"
        android:text="Jadwal Sidang"
        android:textColor="@color/white"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.828"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sidreq"
        app:layout_constraintVertical_bias="0.273" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_jdwlseminar"
        android:layout_width="153dp"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/buttonshapemain"
        android:fontFamily="sans-serif-condensed"
        android:onClick="jdwlseminar"
        android:text="Jadwal Seminar"
        android:textColor="@color/white"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btn_jdwlsidang"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seminarreq"
        app:layout_constraintVertical_bias="0.211" />

    <ImageButton
        android:id="@+id/dosen"
        android:layout_width="52dp"
        android:onClick="dosenPenguji"
        android:layout_height="53dp"
        android:backgroundTint="#ffffff"
        android:src="@drawable/user"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.256"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_jdwlseminar"
        app:layout_constraintVertical_bias="0.613" />

    <ImageButton
        android:id="@+id/prmintaanta"
        android:layout_width="52dp"
        android:layout_height="53dp"
        android:layout_marginEnd="64dp"
        android:backgroundTint="#ffffff"
        android:onClick="listta"
        android:src="@drawable/desk"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.841"
        app:layout_constraintStart_toEndOf="@+id/dosen"
        app:layout_constraintTop_toBottomOf="@+id/btn_jdwlsidang"
        app:layout_constraintVertical_bias="0.582" />

    <ImageButton
        android:id="@+id/logout"
        android:layout_width="33dp"
        android:layout_height="25dp"
        android:backgroundTint="#ffffff"
        android:src="@drawable/logout"
        android:onClick="logout"
        app:layout_constraintBottom_toTopOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.857"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.863" />

</androidx.constraintlayout.widget.ConstraintLayout>